{
  "name": "@assap/assap-sdk",
  "version": "0.1.0",
  "description": "SDK for ASSAP Protocol",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:int": "vitest run --config vitest.integration.config.ts"
  },
  "keywords": [
    "solana",
    "sdk",
    "assap"
  ],
  "author": "ASSAP",
  "license": "MIT",
  "devDependencies": {
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "18.3.20",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.26.0",
    "jsdom": "^26.1.0",
    "lucide-react": "^0.503.0",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metalboyrick/assap"
  },
  "bugs": {
    "url": "https://github.com/metalboyrick/assap/issues"
  },
  "homepage": "https://www.assap.xyz",
  "dependencies": {
    "@civic/auth-web3": "^0.5.8",
    "@civic/solana-gateway-react": "^1.2.4",
    "@coral-xyz/anchor": "^0.30.1",
    "@privy-io/react-auth": "^2.13.1",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-toast": "^1.2.13",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "0.15.36",
    "@solana/wallet-adapter-react-ui": "^0.9.35",
    "@solana/wallet-adapter-wallets": "^0.19.36",
    "@solana/web3.js": "1.98.0",
    "@tanstack/react-query": "^5.75.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0"
  }
}
