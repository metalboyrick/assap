{
  "name": "assap",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "anchor": "turbo run anchor",
    "anchor-build": "turbo run anchor-build",
    "anchor-localnet": "turbo run anchor-localnet",
    "anchor-test": "turbo run anchor-test",
    "ci": "turbo run ci",
    "start": "turbo run start"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.2",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "^18.2.0"
      }
    },
    "overrides": {
      "@types/react": "19.1.4",
      "@types/react-dom": "19.1.5"
    }
  }
}
